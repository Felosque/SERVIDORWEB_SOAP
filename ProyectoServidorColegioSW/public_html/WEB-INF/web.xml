<?xml version = '1.0' encoding = 'windows-1252'?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
         version="3.0">
  <servlet>
    <servlet-name>ServicioEstudianteSWPort</servlet-name>
    <servlet-class>serviciosWeb.ServicioEstudianteSW</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet>
    <servlet-name>ServicioMateriaSWPort</servlet-name>
    <servlet-class>serviciosWeb.ServicioMateriaSW</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet>
    <servlet-name>ServicioMatriculaSWPort</servlet-name>
    <servlet-class>serviciosWeb.ServicioMatriculaSW</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet>
    <servlet-name>ServiciosExtraSWPort</servlet-name>
    <servlet-class>serviciosWeb.ServiciosExtraSW</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>ServicioEstudianteSWPort</servlet-name>
    <url-pattern>/ServicioEstudianteSWPort</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ServicioMateriaSWPort</servlet-name>
    <url-pattern>/ServicioMateriaSWPort</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ServicioMatriculaSWPort</servlet-name>
    <url-pattern>/ServicioMatriculaSWPort</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ServiciosExtraSWPort</servlet-name>
    <url-pattern>/ServiciosExtraSWPort</url-pattern>
  </servlet-mapping>
</web-app>
